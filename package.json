{
  "name": "net-serializer",
  "version": "4.0.0",
  "description": "Type aware object serializer for sending structured data efficiently over network. Uses template object to describe which properties will be serialized into binary data buffer.",
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "node test/index.js",
    "declaration": "tsc --declaration --emitDeclarationOnly",
    "build": "rollup -c && npm run test && npm run declaration"
  },
  "author": "Tatu Anttonen <tatu.anttonen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^3.1.1",
    "lz-string": "^1.4.4",
    "rollup": "^1.32.1",
    "rollup-plugin-terser": "^7.0.1",
    "text-encoding": "^0.7.0",
    "tslib": "^1.13.0",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omenansiemen/net-serializer.git"
  },
  "keywords": [
    "WebRTC",
    "WebSocket",
    "ArrayBuffer",
    "TypedArray",
    "Serialize",
    "Serialization",
    "Flatten",
    "Flattening",
    "Type",
    "Typed",
    "Network"
  ]
}
